<Window x:Class="WpfApp11.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp11"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Lloydbutton" Content="Lloyd" HorizontalAlignment="Center" VerticalAlignment="Center" Width="130" Height="30" Margin="0,0,0,200" FontSize="20" Click="Lloydbutton_Click"/>
        <Button x:Name="Lucindabutton" Content="Lucinda" HorizontalAlignment="Center" VerticalAlignment="Center" Width="130" FontSize="20" Height="30" Margin="0,0,0,50" Click="Lucindabutton_Click"/>
        <Button x:Name="Zamienbutton" Content="Zamień" HorizontalAlignment="Center" VerticalAlignment="Center" Width="130" Height="30" FontSize="20" Margin="0,100,0,0" Click="Zamienbutton_Click"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Center" VerticalAlignment="Center" Width="130" Margin="0,250,0,0" Height="30" FontSize="20" Click="Button_Click"/>

    </Grid>
</Window>
